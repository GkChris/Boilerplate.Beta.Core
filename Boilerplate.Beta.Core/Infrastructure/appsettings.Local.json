{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5432;Database=your_database;User Id=user;Password=password;"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.Authentication": "Debug",
      "Microsoft.AspNetCore.Authorization": "Debug",
      "Microsoft.IdentityModel": "Debug",
      "Microsoft.AspNetCore.Authentication.JwtBearer": "Debug"
    }
  },

  "AllowedHosts": "*",

  "AppSettings": {
    "ApplicationUrl": "http://localhost:5101",
    "ApiBaseUrl": "http://localhost:5101",
    "PageSize": 10
  },

  "Swagger": {
    "Title": "My API",
    "Version": "v1",
    "Description": "API documentation for My API",
    "Contact": {
      "Name": "Your Name",
      "Email": "your.email@example.com",
      "Url": "https://example.com"
    }
  },

  "MetaData": {
    "ApplicationName": "App name"
  },

  "Kafka": {
    "BootstrapServers": "localhost:9093",
    "Topics": [ "example-topic-1", "example-topic-2" ],
    "GroupId": "example-consumer-group"
  },

  "InfrastructureSettings": {
    "AutoApplyMigrations": false,
    "EnableCustomLoggingMiddleware": true,
    "LogExceptionStackTrace": false
  },

  "AuthSettings": {
    "Authority": "http://localhost:9011",
    "Audience": "your-client-id-here",
    "TokenEndpoint": "/oauth2/token",
    "UserInfoEndpoint": "/oauth2/userinfo",
    "IntrospectEndpoint": "/oauth2/introspect",
    "LogoutEndpoint": "/oauth2/logout?client_id=<your-client-id-here>",
    "ClientId": "your-client-id-here",
    "ClientSecret": "your-client-secret-here",
    "RequireHttpsMetadata": false,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true,
    "EnablePostValidation": true,
    "RoleClaim": "roles",
    "ClientName": "FusionAuth"
  }
}